typedef struct structure_LUT
 {
  // 3 tableaux Rouge Vert Bleu
  unsigned char tabR[256];
  unsigned char tabV[256];
  unsigned char tabB[256];
  
  struct structure_LUT* next ;
  struct structure_LUT* previous ;
  int num; //numero operation speciale gris/sepia et nuit
 }LUT;

LUT* creer_init_LUT (void);
int init_LUT (LUT*);
void addcontrast_LUT(LUT*, int);
void dimcontrast_LUT(LUT*, int);
void addLUM_LUT(LUT*, int);
